<?xml version="1.0" encoding="UTF-8"?>
<!-- 
15/07/19 B. Lefebure Update for T2S release 3.2 Partial Release Quantity and Quantity Remaining On-Hold  
08/08/22 B. Lefebure Update for T2S release 6.2 Add Counterparty MITI and InputMediaID
-->
<xs:schema xmlns:cst2s="cst2s.schema.clearstream" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:t2s="urn:iso:std:iso:20022:tech:xsd:head.001.001.01" targetNamespace="cst2s.schema.clearstream" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!-- header import for embedded messages -->
	<xs:import namespace="urn:iso:std:iso:20022:tech:xsd:head.001.001.01" schemaLocation="ISO_T2S_Xml/head.001.xsd"/>
	<xs:element name="CST2SMsg">
		<xs:annotation>
			<xs:documentation>Root element: Clearstream - T2S Message</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CSPayload">
					<xs:annotation>
						<xs:documentation>Clearstream payload first
In this node, all Clearstream "plus" information is collected.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="IntApplHead" type="cst2s:IntApplHdrType"/>
							<xs:element name="StrmgInfo" type="cst2s:StrmgInfoType" minOccurs="0"/>
							<xs:element name="MsgProcInfo" minOccurs="0" maxOccurs="4">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="InxTyp" type="cst2s:MaxChar4Typ">
											<xs:annotation>
												<xs:documentation>Defines if the sequence contains original or related information</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="InxRef">
											<xs:annotation>
												<xs:documentation>Contains referencei information to link feedback to original message.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="OrigSEME" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Original customer reference created for cancellations and modifications</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CBFMLMTxnId" type="cst2s:RestrictedFINXMax50Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>MLM identifier of the business message</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CSTxnId" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Clearstream reference: Clearstream-wide unique indentifier assigned by the first system to deal with the messages. Common ID used by CBL systems.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="MktInfrstrctrTxId" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>T2S Reference</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="T2SActrRef" type="cst2s:T2SActrRefType" minOccurs="0" maxOccurs="2">
														<xs:annotation>
															<xs:documentation>Repetitive sequence to list all available T2S Actor Reference, together  with the corresponding reference owners. T2S Actor references are only relevant in case a sese.023 or semt.013 is refered to</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="T2STxRef" type="cst2s:RestrictedFINXMax16Text" minOccurs="0"/>
													<xs:element name="CxlReqRef" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Contains the reference of the original sese.020</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ReqRef" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Contains the reference of the original reda.030</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ReqMsgId" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Contains the reference of the original creation or modificationr equest</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="QryRef" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Contains the reference of the original business query which triggered the report</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Pagtn" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Pagination details</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="PgNbr" type="cst2s:Max5NumericText">
																	<xs:annotation>
																		<xs:documentation>Specifies the Page Number</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="LstPgInd" type="t2s:YesNoIndicator">
																	<xs:annotation>
																		<xs:documentation>Specifies if the page is the last page</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="BizMsgIdr" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Copy of T2S header</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="MtchLegRef" type="cst2s:MtchLegRefType" minOccurs="0" maxOccurs="3">
														<xs:annotation>
															<xs:documentation>This subsequence contains the references that allow to identify the matched leg.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CtrPtyMktInfrstrctrTxId" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>T2S Reference of Counterparty Instruction</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="InxDetl" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Contains fixed and unchangeable attributes of the references</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SctiesMvmntTp" type="cst2s:ReceiveDelivery1Code" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Copy from SttlmTpAndAddtlParams/SctiesMvmntTp</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="MsgDefIdr" type="t2s:MsgNmId_T2S_1">
														<xs:annotation>
															<xs:documentation>Enforce type with ISO15022 MsgNmId_T2S_1</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="InstructgMode" type="cst2s:InstructgModeType">
														<xs:annotation>
															<xs:documentation>DCP/ICP/Instructing generated by T2S</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="GenSystNm" type="cst2s:MaxChar4Typ" minOccurs="0">
														<xs:annotation>
															<xs:documentation>System where the referenced payload message was first generated</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TLMGenFlg" type="t2s:YesNoIndicator" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Indicated if the message was generated by TLM</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TLMGenReasonCd" type="cst2s:TLMGenReasonCdType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Reason code why TLM generated the instruction</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="T2SAckTmstmp" type="cst2s:ISONormalisedDateTime" minOccurs="0">
														<xs:annotation>
															<xs:documentation>T2S internal timestamp for accepted status</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="T2SMtchTmstmp" type="cst2s:ISONormalisedDateTime" minOccurs="0">
														<xs:annotation>
															<xs:documentation>T2S internal timestamp for matched status</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ModCxlMvmntTp" type="cst2s:ReceiveDelivery1Code" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Specifies movement type of modified or cancelled instruction leg</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="GenRsn" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Optional reason indicating purpose of the generated instruction</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="InputMediaId" type="cst2s:MaxChar16Typ" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Input MediaId of instruction</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="NoLifecycChgFlg" type="t2s:YesNoIndicator" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Indicates in case a trigger message from T2S does not result in fa lifecycle change</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="NoLifecycChgReason" type="cst2s:NoLifecycChgReasonType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Reason why the trigger message from T2S does not result in a lifecyle change</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="InsLifecycChg" type="cst2s:InsLifecycChgType" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Contains all changes to the lifecycle</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="IntAddInfo" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Additional attributes to the flow between LIMA/SPOC (for input from CASCADE) and PM, should such a need arise in the future</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="IntCancReason.list" minOccurs="0">
														<xs:complexType>
															<xs:sequence maxOccurs="unbounded">
																<xs:element name="LifecycReason" type="cst2s:ReasonType">
																	<xs:annotation>
																		<xs:documentation>Business reason why an instruction needs to be cancelled</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="IntRjctReason.list" minOccurs="0">
														<xs:complexType>
															<xs:sequence maxOccurs="unbounded">
																<xs:element name="LifecycReason" type="cst2s:ReasonType">
																	<xs:annotation>
																		<xs:documentation>Business reason why an ICP instruction was rejected by the MLM</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="OrigCustMsgInfo" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Contains the original customer message</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="OrigMsgTyp" type="cst2s:MaxChar8Typ">
																	<xs:annotation>
																		<xs:documentation>Contains the message type, e.g. MT543, CIF...</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="OrigMsgCtnt" type="xs:anyType">
																	<xs:annotation>
																		<xs:documentation>Will contain the original customer message as free text</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="AddMve" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Additional movements requested by originating system original customer message</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="PosTyp" type="cst2s:PosTypType">
																	<xs:annotation>
																		<xs:documentation>Contains the position type of the additional movement</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="T2SPayload" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AppHdr" type="t2s:BusinessApplicationHeaderV01"/>
							<xs:group ref="cst2s:origMsg"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ErrHdlg" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Error andling block</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="RelMsgDetl" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Contains the messagetype and the message sent to T2S if one exists</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="RelMsgDefIdr" type="t2s:MsgNmId_T2S_1" minOccurs="0">
											<xs:annotation>
												<xs:documentation>To indicate the message type, of the message sent to T2S causing an error</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="RelMsgCtnt" type="xs:anyType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Will contain the message sent towards T2S causing the error</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="SystErrDetl" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="SystErr" type="cst2s:RestrictedFINXMax50Text" minOccurs="0" maxOccurs="10">
											<xs:annotation>
												<xs:documentation>Up to 10 fields which describe the error. Filled by the backend. Should be filled with qualifier and value.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="TargetBusTyp" type="cst2s:RestrictedFINXMax50Text" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The backend can specify an additional routing criteria used to identify the PI² target unit</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="IntApplHdrType">
		<xs:annotation>
			<xs:documentation>Complex Type Application Header</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ApplFrom" type="cst2s:ApplType"/>
			<xs:element name="ApplTo" type="cst2s:ApplType"/>
			<xs:element name="TechMsgId" type="cst2s:RestrictedFINXMax16Text"/>
			<xs:element name="MsgDefIdr" type="t2s:MsgNmId_T2S_1"/>
			<xs:element name="CreDt" type="cst2s:ISONormalisedDateTime"/>
			<xs:element name="CSRecvTmstmp" type="cst2s:ISONormalisedDateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Timestamp when the original trigger for this message (e.g. MT from customer, MX from T2S) entered the Clearstream system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CpyDplct" type="t2s:CopyDuplicate1Code" minOccurs="0"/>
			<xs:element name="PssblDplct" type="t2s:YesNoIndicator" minOccurs="0"/>
			<xs:element name="CSPrioOrder" type="cst2s:PrioOrderType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Processing precedence</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="T2SSeqNbr" type="t2s:Max16NumericText_T2S" minOccurs="0"/>
			<xs:element name="RecvFileInd" type="t2s:YesNoIndicator" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates wether the input from T2S was received in a single message or a file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IgnBndlFlg" type="t2s:YesNoIndicator" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicator set to Y if the message should bot be considered for bundling in files by T2S-I</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InsLifecycChgType">
		<xs:annotation>
			<xs:documentation>Complex Type Instruction Lifecycle Change</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TargetSystSeqNbr" type="cst2s:Max16NumericText">
				<xs:annotation>
					<xs:documentation>Target system specific sequence number that defines the occurence of the update in the lifecycle. It must be strictly ascending with no gaps.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="T2SSeqNbr" type="t2s:Max16NumericText_T2S">
				<xs:annotation>
					<xs:documentation>Sequence number provided by T2S</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TLMStatusTmstmp" type="cst2s:ISONormalisedDateTime">
				<xs:annotation>
					<xs:documentation>Internal Timestamp assigned by TLM</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TLMActvTyp" type="cst2s:RestrictedFINXMax35Text">
				<xs:annotation>
					<xs:documentation>TLM functional activity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TLMActvState" type="cst2s:RestrictedFINXMax35Text">
				<xs:annotation>
					<xs:documentation>TLM functional activity status</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LifecycReason" type="cst2s:ReasonType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="MtchDetl" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains detailed information about attributes changed due to matching activities</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SttlmAmt" type="cst2s:RestrictedFINActiveCurrencyAndAmount" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Contains the adjusted settlement amount after matching (on receiver side only)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="SettleDetl" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains detailed information about attributes changed due to settlement activities</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="FctvSttlmDt" type="cst2s:ISONormalisedDateTime"/>
						<xs:element name="SttldQty" type="cst2s:RestrictedFINDecimalNumber" minOccurs="0"/>
						<xs:element name="PrevslySttldQty" type="cst2s:RestrictedFINDecimalNumber" minOccurs="0"/>
						<xs:element name="RmngToBeSttldQty" type="cst2s:RestrictedFINDecimalNumber" minOccurs="0"/>
						<xs:element name="SttldAmt" type="cst2s:RestrictedFINActiveCurrencyAndAmount" minOccurs="0"/>
						<xs:element name="PrevslySttldAmt" type="cst2s:RestrictedFINActiveCurrencyAndAmount" minOccurs="0"/>
						<xs:element name="RmngToBeSttldAmt" type="cst2s:RestrictedFINActiveCurrencyAndAmount" minOccurs="0"/>
						<xs:element name="RestrictRef" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This is the reference of the restriction which was used to settle this instruction </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="LnkDetl" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains all information regarding linkage</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PrcgPos" type="cst2s:ProcessingPosition3Choice">
							<xs:annotation>
								<xs:documentation>Linkage position type</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MktInfrstrctrTxId" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
							<xs:annotation>
								<xs:documentation>T2S Reference</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="T2SActrRef" type="cst2s:T2SActrRefType" minOccurs="0" maxOccurs="2"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PartRlsDetl" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains detailed information about quantities affected by partial release</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PartRlsQty" type="cst2s:RestrictedFINDecimalNumber" minOccurs="1">
							<xs:annotation>
								<xs:documentation>Partially Released Quantity as communicated by T2S</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="HoldRmngQty" type="cst2s:RestrictedFINDecimalNumber" minOccurs="1">
							<xs:annotation>
								<xs:documentation>Quantity Remaining on Hold as communicated by T2S</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>												
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MtchLegRefType">
		<xs:annotation>
			<xs:documentation>Complex Type Matched Leg Reference</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CBFMLMTxnId" type="cst2s:RestrictedFINXMax50Text" minOccurs="0">
				<xs:annotation>
					<xs:documentation>MLM identifier of the business message</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CSTxnId" type="cst2s:RestrictedFINXMax16Text" minOccurs="0">
				<xs:annotation>
